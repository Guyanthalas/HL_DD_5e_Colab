    <bootstrap thing="ra5CSddnCcph"></bootstrap>
    <bootstrap thing="ra5CVoicBynd"></bootstrap>
    </thing>
  <thing id="ra5CVRGRSpWh" name="Spirit Whispers" description="You learn the {i}message{/i} cantrip, and can cast it without any components. Messages are delivered by one of your whispering spirits. Choose Intelligence, Wisdom, or Charisma as your spellcasting ability for this spell when you gain this Dark Gift." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.BaseAttr &amp; (IsAttr.aWIS | IsAttr.aCHA | IsAttr.aINT)"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="Helper" tag="ActivMenu"/>
    <bootstrap thing="spMessage">
      <autotag group="Helper" tag="SpellLike"/>
      </bootstrap>
    </thing>
  <thing id="ra5CSddnCcph" name="Sudden Cacophony" description="You can use your reaction when an attack hits you to let your haunting spirits howl through you. If the attacker can hear you, your proficiency bonus is added to your AC against that attack. Once this feature has caused an attack to miss, you cannot use it again until you complete a long rest." compset="RaceSpec">
    <fieldval field="trkMax" value="1"/>
    <tag group="abAction" tag="Reaction"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="LongRest"/>
    <tag group="FeatureTyp" tag="Reaction"/>
    </thing>
  <thing id="ra5CVoicBynd" name="Voices From Beyond" description="The first time after a short or long rest that you roll a 1 on the d20 for an attack roll, ability check, or saving throw, the voices that haunt you become impossible to ignore. Roll on the below table to determine the effect theses voices impose on you.\n\n{b}Voices from Beyond{/b}\n{font Courier New}{back FFFFFF}d4  Effect\n{back 010101}1   Your next attack roll, ability check, or saving throw is made with disadvantage.\n{back FFFFFF}2   Voices deafen you for 1 minute.\n{back 010101}3   You are frightened of the creature nearest you, other than yourself, until the end of your next turn. If multiple creatures are closest, the DM chooses which one becomes the object of your fear.\n{back FFFFFF}4   Within the next 10 minutes, you can ask your spirits about a specific course of action that you plan to take within the next 30 minutes. You can receive an omen about the results, as if you had cast {i}augury{/i}. The omen manifests as whisperings from your spirits, which nobody else can perceive.\n{back 010101}" compset="RaceSpec">
    <fieldval field="trkMax" value="1"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Usage" tag="ShortRest"/>
    </thing>
  <thing id="ra5CGrspShdw" name="Grasping Shadow" description="You learn the {i}mage hand{/i} cantrip, and can cast it without any components. The hand appears to be made of shadow but is not bound to yours. Choose Intelligence, Wisdom, or Charisma as your spellcasting ability for this spell when you gain this Dark Gift." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.BaseAttr &amp; (IsAttr.aWIS | IsAttr.aCHA | IsAttr.aINT)"/>
    <tag group="ChooseSrc1" tag="Hero"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="Helper" tag="ActivMenu"/>
    <bootstrap thing="spMageHand">
      <autotag group="Helper" tag="SpellLike"/>
      </bootstrap>
    </thing>
  <thing id="ra5CShdwStrk" name="Shadow Strike" description="You can increase your reach by 10 feet for a number of attacks equal to your proficiency bonus, as your shadow stretches and delivers the attack as if it were you. You regain these uses upon finishing a long rest." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Usage" tag="LongRest"/>
    <eval phase="PostAttr"><![CDATA[
    doneif (tagis[Helper.Disable] <> 0)


    field[trkMax].value = hero.childfound[ProfBonus].field[tProfBonus].value]]></eval>
    </thing>
  <thing id="ra5COmnsWill" name="Ominous Will" description="The first time after a short or long rest that you roll a 1 on the d20, your shadow exerts its own will. The next time a creature you can see within 30 feet of you (including yourself) makes an attack roll, an ability check, or a saving throw, roll a d4. If the result is odd, reduce the d20 result by that number. If it&apos;s even, increase the d20 result by that much." compset="RaceSpec">
    <fieldval field="trkMax" value="1"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="Usage" tag="ShortRest"/>
    </thing>
  <thing id="c5CLvgShd" name="Living Shadow" description="Your shadow is always visible and has a mind of its own, moving independently from you. Roll or choose from the Shadow Quirk table." compset="CustomSpec" summary="Your shadow is always visible and moves on its own." uniqueness="unique">
    <usesource source="5eVRGtR"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="Helper" tag="Quaternary"/>
    <tag group="AllowRCust" tag="cfg5CSupGif" name="Dark Gifts" abbrev="Dark Gifts"/>
    <bootstrap thing="ra5CGrspShdw"></bootstrap>
    <bootstrap thing="ra5CShdChr"></bootstrap>
    <bootstrap thing="ra5CShdwStrk"></bootstrap>
    <bootstrap thing="ra5COmnsWill"></bootstrap>
    </thing>
  <thing id="ra5CMistStep" name="Misty Step" description="You learn the {i}misty step{/i} spell, and can cast it without using a spell slot. You may also cast this spell with spell slots of 2nd level or higher. Choose Intelligence, Wisdom, or Charisma as your spellcasting ability for this spell when you gain this Dark Gift." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.BaseAttr &amp; (IsAttr.aWIS | IsAttr.aCHA | IsAttr.aINT)"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <tag group="abAction" tag="None" name="No action" abbrev="None"/>
    <tag group="Helper" tag="ActivMenu" name="ActivMenu" abbrev="ActivMenu"/>
    <tag group="Custom" tag="OnlyIfCaster" name="OnlyIfCaster" abbrev="OnlyIfCaster"/>
    <bootstrap thing="spMistStep">
      <autotag group="Helper" tag="Free"/>
      <autotag group="Helper" tag="Memorized"/>
      <autotag group="Hide" tag="Spell"/>
      <autotag group="Custom" tag="OnlyIfCaster"/>
      </bootstrap>
    <bootstrap thing="spMistStep">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Usage" tag="LongRest"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <eval phase="Final" priority="10000"><![CDATA[var i2nd as number

if (hero.childfound[SlotHold2].field[MaxAllow].value > 0) then
  i2nd += 1
  endif

if (#levelcount[Warlock] >= 3) then
  i2nd += 1
  endif


doneif (i2nd = 0)

foreach pick in hero from BaseSpell where "Custom.OnlyIfCaster"
  perform eachpick.delete[Hide.Spell]
  nexteach]]></eval>
    </thing>
  <thing id="ra5CMistTrav" name="Mist Traveler" description="When traversing the Mists to reach a particular domain, you are treated as having a Mist talisman keyed to that domain. You need not have visited that domain before, only know its name, however if the domain borders were closed by a Darklord&apos;s will, you cannot bypass those borders." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CPsnRoots" name="Poisoned Roots" description="Whenever you finish a long rest, a 10 mile area around where you rested becomes slightly hazardous to you. You may remain in that area safely for a number of weeks equal to your Constitution modifier (minimum 1) . Every time you complete a long rest after that period, you must make a DC 15 Constitution saving throw or gain a level of exhaustion. This level of exhaustion cannot be removed until you leave the area." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="LvNamePar" tag="AppText"/>
    <eval phase="Final"><![CDATA[field[abValue].value = hero.childfound[aCON].field[aModBonus].value

field[abText].text = "DC 15 CON, " & field[abValue].value & " weeks"]]></eval>
    </thing>
  <thing id="c5CMstWlk" name="Mist Walker" description="You know how to navigate the Mists, but can&apos;t remain in one place for too long. Roll or choose from the Misty Travels table." compset="CustomSpec" summary="You are adept at traveling the Mists." uniqueness="unique">
    <usesource source="5eVRGtR"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="Helper" tag="Quaternary"/>
    <tag group="AllowRCust" tag="cfg5CSupGif" name="Dark Gifts" abbrev="Dark Gifts"/>
    <bootstrap thing="ra5CWkrChr"></bootstrap>
    <bootstrap thing="ra5CMistStep"></bootstrap>
    <bootstrap thing="ra5CMistTrav"></bootstrap>
    <bootstrap thing="ra5CPsnRoots"></bootstrap>
    </thing>
  <thing id="ra5CTransform" name="Transformation" description="You learn the {i}alter self{/i} spell, and can cast it once per long rest without using a spell slot. You may also cast this spell with spell slots of 2nd level or higher. When casting this spell, you may only use the spell&apos;s Change Appearance option, and only change into your second form. When you transform back to your normal form, you retain some aspect of your second form until you complete a long rest, such as scaly skin, stunted wings, eyes without pupils, horns, etc.. Choose Intelligence, Wisdom, or Charisma as your spellcasting ability for this spell when you gain this Dark Gift." compset="RaceSpec">
    <fieldval field="usrCandid1" value="component.BaseAttr &amp; (IsAttr.aWIS | IsAttr.aCHA | IsAttr.aINT)"/>
    <fieldval field="trkMax" value="1"/>
    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <tag group="abAction" tag="None" name="No action" abbrev="None"/>
    <tag group="Helper" tag="ActivMenu" name="ActivMenu" abbrev="ActivMenu"/>
    <tag group="Custom" tag="OnlyIfCaster" name="OnlyIfCaster" abbrev="OnlyIfCaster"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="Usage" tag="LongRest"/>
    <bootstrap thing="spAlteSelf">
      <autotag group="Helper" tag="Memorized"/>
      <autotag group="Hide" tag="Spell"/>
      <autotag group="Custom" tag="OnlyIfCaster"/>
      <autotag group="Helper" tag="Free"/>
      <assignval field="sNameMod" value="Your second form only"/>
      </bootstrap>
    <bootstrap thing="spAlteSelf">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Usage" tag="LongRest"/>
      <assignval field="trkMax" value="1"/>
      <assignval field="sNameMod" value="Your second form only"/>
      </bootstrap>
    <eval phase="Final" priority="10000"><![CDATA[var i2nd as number

if (hero.childfound[SlotHold2].field[MaxAllow].value > 0) then
  i2nd += 1

debug "Slot 2's found"
  endif

if (#levelcount[Warlock] >= 3) then
  i2nd += 1
debug "warlock found"
  endif


doneif (i2nd = 0)
debug "not done"
foreach pick in hero from BaseSpell where "Custom.OnlyIfCaster"
  perform eachpick.delete[Hide.Spell]
  debug eachpick.field[livename].text
  nexteach]]></eval>
    </thing>
  <thing id="ra5CInvolChg" name="Involuntary Change" description="Specific situations trigger your Dark Gift. At the start of your next turn after your Dark Gift is triggered in this way, you must make a DC 15 Charisma saving throw or cast {i}alter self{/i} as described in the Transformation trait, even if you cannot normally use it again. Roll or choose from the Change Catalyst table to see what triggers the change.\n\n{b}Change Catalyst{/b}\n{font Courier New}{back FFFFFF}d6  Catalyst\n{back 010101}1   Seeing a specific phase of the moon\n{back FFFFFF}2   A certain type of flower&apos;s smell\n{back 010101}3   The sound of a temple&apos;s bells being rung.\n{back FFFFFF}4   Hearing a specific melody\n{back 010101}5   Touching pure silver with unprotected skin\n{back FFFFFF}6   Seeing someone who resembles a specific individual\n{back 010101}" compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="1-Specific moon phase"/>
    <arrayval field="usrArray" index="1" value="2-Certain flower&apos;s smell"/>
    <arrayval field="usrArray" index="2" value="3-Ringing temple bells"/>
    <arrayval field="usrArray" index="3" value="4-Particular melody"/>
    <arrayval field="usrArray" index="4" value="5-Touching pure silver"/>
    <arrayval field="usrArray" index="5" value="6-Specific person&apos;s appearance"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    </thing>
  <thing id="c5CSecSkn" name="Second Skin" description="You can transform into a second, very different form. Roll or choose from the Second Form table." compset="CustomSpec" summary="You have a second form that differs greatly from your own." uniqueness="unique">
    <usesource source="5eVRGtR"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="Helper" tag="Quaternary"/>
    <tag group="AllowRCust" tag="cfg5CSupGif" name="Dark Gifts" abbrev="Dark Gifts"/>
    <bootstrap thing="ra5CTransform"></bootstrap>
    <bootstrap thing="ra5CSknChr"></bootstrap>
    <bootstrap thing="ra5CInvolChg"></bootstrap>
    </thing>
  <thing id="c5CSmbBng" name="Symbiotic Being" description="A symbiotic being is merged with you and depends on you for survival. Roll or choose from the Symbiotic Nature table." compset="CustomSpec" summary="You host a symbiote." uniqueness="unique">
    <usesource source="5eVRGtR"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="Helper" tag="Quaternary"/>
    <tag group="AllowRCust" tag="cfg5CSupGif" name="Dark Gifts" abbrev="Dark Gifts"/>
    <bootstrap thing="cfg5CSymbio"></bootstrap>
    </thing>
  <thing id="c5CSustSymb" name="Sustained Symbiosis" description="Your symbiote takes extra measures to keep you alive. If you fail a saving throw, it can expend one of your Hit Dice, rolls it, and adds the rseult to your saving throw. If it does this for a death saving throw, it doesn&apos;t matter what you rolled on the d20, you automatically succeed and regain 1 hit point. Once your symbiote helps you succeed on one saving throw, it cannot assist in this manner again until you finish a long rest." compset="CustomSpec">
    <tag group="Helper" tag="Secondary"/>
    <tag group="CustTaken" tag="cfg5CSymbio"/>
    </thing>
  <thing id="c5CSymbAgnd" name="Symbiotic Agenda" description="Your symbiote has some driving goal, and it wants you to help accomplish it. Your DM determines how patient or permissive it is, as that depends on its personality, but if you have an opportunity to help further its cause and you don&apos;t act on it, the symbiote can try to charm you into doing so. You must succeed on a Charisma saving throw (DC) or become charmed and follow its commands for 1d12 hours. Taking damage that&apos;s not self-inflicted allows you to make another saving throw, ending the effect on a success. Roll or choose from the Symbiotic Agenda table.\n\n{b}Symbiotic Agenda{/b}\n{font Courier New}{back FFFFFF}d6  Agenda\n{back 010101}1   The symbiote seeks to destroy a specific type of creature, such as dragons, gnolls, or clerics.\n{back FFFFFF}2   The symbiote desires knowledge and is driven to solve a mystery or learn a secret.\n{back 010101}3   The symbiote desires to either make true or thwart a prophecy.\n{back FFFFFF}4   The symbiote wants to defend the members and interests of a powerful entity or organization.\n{back 010101}5   The symbiote seeks your safety, and insists it knows what&apos;s best for you.\n{back FFFFFF}6   The symbiote wants new experiences, the stranger the better.\n{back 010101}" compset="CustomSpec">
    <arrayval field="usrArray" index="0" value="1-Specific type of creature"/>
    <arrayval field="usrArray" index="1" value="2-Knowledge, secrets, mystery"/>
    <arrayval field="usrArray" index="2" value="3-Prophecy"/>
    <arrayval field="usrArray" index="3" value="4-A being or organization"/>
    <arrayval field="usrArray" index="4" value="5-Your best interest"/>
    <arrayval field="usrArray" index="5" value="6-New, strange experiences"/>
    <arrayval field="usrArray" index="6" value="7-Other (displays text box)"/>
    <tag group="CustTaken" tag="cfg5CSymbio"/>
    <tag group="Helper" tag="Secondary"/>
    <tag group="LvNamePar" tag="AppText"/>
    <tag group="User" tag="NoAutoName"/>
    <eval phase="Final" priority="150"><![CDATA[field[abValue2].value = hero.childfound[c5CSymbCha].field[abValue].value + 12
field[abText].text = "DC " & field[abValue2].value & " CHA" & field[abText2].text]]></eval>
    <eval phase="Final" index="2"><![CDATA[if (field[usrIndex].value = 0) then
  field[abText2].text = ", Creature"
elseif (field[usrIndex].value = 1) then
  field[abText2].text = ", Knowledge"
elseif (field[usrIndex].value = 2) then
  field[abText2].text = ", Prophecy"
elseif (field[usrIndex].value = 3) then
  field[abText2].text = ", Entity"
elseif (field[usrIndex].value = 4) then
  field[abText2].text = ", You"
elseif (field[usrIndex].value = 5) then
  field[abText2].text = ", Experiences"
elseif (field[usrIndex].value = 6) then
  perform assign[User.NeedDomain]
  field[abText2].text = ", " & field[domDomain].text
endif]]></eval>
    </thing>
  <thing id="ra5CDeathTch" name="Death Touch" description="Make an unarmed strike as an action. In addition to the normal damage, you deal 1d10 necrotic damage, 2d10 necrotic damage if you&apos;re at least 5th level, 3d10 if you&apos;re at least 11th level, or 4d10 if you&apos;re 17th level." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="LvNamePar" tag="AppText"/>
    <tag group="FeatureTyp" tag="Action"/>
    <tag group="abAction" tag="Action"/>
    <eval phase="Final"><![CDATA[doneif (tagis[Helper.Disable] <> 0)

if (hero.tagcount[Classes.?] > 16) then
   field[abValue].value = 4
elseif (hero.tagcount[Classes.?] > 10) then
   field[abValue].value = 3
elseif (hero.tagcount[Classes.?] > 4) then
   field[abValue].value = 2
else
   field[abValue].value = 1
endif

field[abText].text = "Unarmed strike + " & field[abValue].value & "d10 necrotic"]]></eval>
    </thing>
  <thing id="ra5CInescDth" name="Inescapable Death" description="You ignore a target&apos;s resistance to necrotic damage when you hit it with an attack roll and deal necrotic damage." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    </thing>
  <thing id="ra5CWithCont" name="Withering Contact" description="If you start a turn either grapling or being grappled by another creature, that creature takes 1d10 necrotic damage." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    </thing>
  <thing id="c5CTchDth" name="Touch of Death" description="Physical contact with you harms others. Roll or choose from the Deadly Touch table." compset="CustomSpec" summary="You exude necrotic energy." uniqueness="unique">
    <usesource source="5eVRGtR"/>
