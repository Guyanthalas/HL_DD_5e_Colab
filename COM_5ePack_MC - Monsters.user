    <tag group="abSave" tag="aCON"/>
    </thing>
  <thing id="ra5CFrcScryFcs" name="Scrying Focus" description="A spellcaster can use the fractine as a focus when casting the {i}scrying{/i} spell or similar magic, so long as the spellcaster and the fractine are within 5 feet of each other." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CFrcSplit" name="Split" description="When a Large fractine that has at least 10 hit points remaining takes bludgeoning, piercing, slashing, or thunder damage from any source, it is split into two Medium fractines. The new fractines occupy the space formerly occupied by the original fractine, and each new fractine has hit points equal to half the original&apos;s, rounded down. If the original fractine had a creature trapped in its demiplane, that creature is released when the fractine splits, reappearing in an open space as near the new fractines as possible." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Reaction"/>
    <tag group="abAction" tag="Reaction"/>
    </thing>
  <thing id="ra5CFrc2Dmnsn" name="Two-Dimensionality" description="The fractine can occupy another creature&apos;s space and vice versa. It can move through other creatures and objects as if they were difficult terrain, but it suffers 5 (1d10) force damage if it ends its turn inside an object." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CGdbAirEnv" name="Air Envelope" description="If it has at least 1 hit point, the gadabout can create an air envelope around itself while in a vacuum. This air envelope can sustain the gadabout and one other creature in its space indefinitely." compset="RaceSpec">
