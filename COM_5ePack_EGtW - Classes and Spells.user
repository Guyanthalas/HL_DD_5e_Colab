    <tag group="sComp" tag="S"/>
    <tag group="sComp" tag="M"/>
    <tag group="Helper" tag="NoAdvLeag"/>
    <tag group="Helper" tag="SplSave"/>
    </thing>
  <thing id="sp5CWldGrS" name="Gravity Sinkhole" description="You create a 20-foot-radius sphere at a point visible to you within range. Every creature within the sphere must make a Constitution save. On a failure, a creature suffers 5d10 force damage and is pulled directly towards the center of the sphere, ending in an open space as close to that center as possible (even if that&apos;s in the air). On a successful save, the creature takes half the damage and is not pulled.\n\n{b}{i}At Higher Levels.{/i}{/b} The damage of the spell increases by 1d10 for each slot above 4th level used to cast it." compset="Spell">
    <fieldval field="sCompDesc" value="a black marble"/>
    <fieldval field="sRange" value="120 feet"/>
    <fieldval field="sDuration" value="Instantaneous"/>
    <usesource source="5eEGtWCP" parent="UserParent"/>
    <tag group="sLevel" tag="4"/>
