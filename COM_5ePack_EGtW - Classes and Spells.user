    <tag group="sComp" tag="M"/>
    <tag group="Helper" tag="NoAdvLeag"/>
    <tag group="sLevel" tag="2"/>
    <tag group="Helper" tag="SplMatAtLe"/>
    </thing>
  <thing id="sp5CWldGio" name="Gift of Alacrity" description="You touch a willing creature, granting it 1d8 to its initiative rolls for the duration of the spell." compset="Spell">
    <fieldval field="sRange" value="Touch"/>
    <fieldval field="sDuration" value="8 hours"/>
    <usesource source="5eEGtWCP" parent="UserParent"/>
