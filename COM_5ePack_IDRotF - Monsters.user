    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CSptMAcd" name="Spit Acid" description="The mimic spits acid at one creature visivle to and within 30 feet of it. The target must make a DC 14 Dexterity save. It suffers 32 (9d6 + 1) acid damage on failure or half that on a success." compset="RaceSpec">
