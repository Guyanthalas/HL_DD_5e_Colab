      <autotag group="ChargeUse" tag="1"/>
      <assignval field="sNameMod" value="bonus action, touch"/>
      </bootstrap>
    <bootstrap thing="w5CBlkstff">
      <containerreq phase="First" priority="100"><![CDATA[        fieldval:gIsAttuned <> 0 & fieldval:gIsEquip <> 0]]></containerreq>
      </bootstrap>
    <eval phase="PreLevel" priority="10000"><![CDATA[doneif (field[gIsEquip].value = 0)
doneif (field[gIsAttuned].value = 0)

      field[abText].text = "regains 2d8+4 at dawn"
      hero.child[svAll].field[Bonus].value += 2

      perform assign[Helper.ShowSpec]

      foreach pick in hero from Class
         eachpick.field[cSplAttack].value += 2
         nexteach

      hero.child[ArmorClass].field[Bonus].value += 2]]></eval>
    </thing>
  <thing id="io5CBrFlyDg" name="Bracer of Flying Daggers" description="This armband appears to hold thin daggers. You can use an action to draw up to two magic daggers from the bracer and immediately throw them, making a ranged attack with each dagger. A dagger vanishes if you don&apos;t throw it right away, and the daggers disappear right after they hit or miss. The bracer has an infinite supply of such daggers." compset="Wondrous">
