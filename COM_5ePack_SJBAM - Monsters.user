  <thing id="p5CAstrlVar" name="Astral Variant" description="Add this adjustment to any creature to make it an Astral Variant, with this feature:\n\n{b}Unusual Nature.{/b} This creature doesn&apos;t need air." compset="InPlay">
    <usesource source="5eBooAst"/>
    <tag group="Helper" tag="NoIncr" name="NoIncr" abbrev="NoIncr"/>
    <tag group="OthAdjCat" tag="UserBot5th" name="User 5th Bottom Group" abbrev="User 5th Bottom Group"/>
    <tag group="Helper" tag="AdjInfo" name="Adjustment Information" abbrev="Adjustment Information"/>
    <bootstrap thing="ra5CUnNatAir">
      <containerreq phase="First" priority="101"><![CDATA[fieldval:pIsOn <> 0]]></containerreq>
      <assignval field="abText" value="This creature"/>
      </bootstrap>
    <eval phase="First"><![CDATA[doneif (field[pIsOn].value = 0)
doneif (field[pChosen].ischosen = 0)

if (field[pChosen].chosen.tagis[thingid.ra5CMagResN] <> 0) then
  field[pMaximum].value = 1
elseif (field[pChosen].chosen.tagis[thingid.ra5CLighSe] <> 0) then
  field[pMaximum].value = 2
elseif (field[pChosen].chosen.tagis[thingid.ra5CImmFor] <> 0) then
  field[pMaximum].value = 3
elseif (field[pChosen].chosen.tagis[thingid.ra5CMagWea] <> 0) then
  field[pMaximum].value = 4
elseif (field[pChosen].chosen.tagis[thingid.ra5CNyxStp] <> 0) then
  field[pMaximum].value = 5
elseif (field[pChosen].chosen.tagis[thingid.ra5CStrltFrm] <> 0) then
  field[pMaximum].value = 6
  endif]]></eval>
    </thing>
