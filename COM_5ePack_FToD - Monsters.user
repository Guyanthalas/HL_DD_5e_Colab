  <thing id="ra5CABrChrFor" name="Chromatic Form" description="When created, the animated breath takes a form that matches its creator&apos;s breath weapon: Acid, Cold, Fire, Lightning, or Poison. This form determines the creature&apos;s AC, damage resistance, traits, and attacks." compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="Acid"/>
    <arrayval field="usrArray" index="1" value="Cold"/>
    <arrayval field="usrArray" index="2" value="Fire"/>
    <arrayval field="usrArray" index="3" value="Lightning"/>
    <arrayval field="usrArray" index="4" value="Poison"/>
    <tag group="abAction" tag="None"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <bootstrap thing="ra5CABrFirAur">
      <containerreq phase="First" priority="100">fieldval:usrIndex = 2</containerreq>
      </bootstrap>
    <bootstrap thing="ra5CABrPutAur">
      <containerreq phase="First" priority="100">fieldval:usrIndex = 0 | fieldval:usrIndex = 4</containerreq>
      </bootstrap>
    <bootstrap thing="ra5CABrLigBur">
      <containerreq phase="First" priority="100">fieldval:usrIndex = 3</containerreq>
      </bootstrap>
    <eval phase="First">if (field[usrIndex].value = 0) then
    perform hero.assign[DamageRes.dtAcid]
elseif (field[usrIndex].value = 1) then
    perform hero.assign[DamageRes.dtCold]
    hero.childfound[r5CAnmBrth].field[rAC].value += 2
elseif (field[usrIndex].value = 2) then
    perform hero.assign[DamageRes.dtFire]
elseif (field[usrIndex].value = 3) then
    perform hero.assign[DamageRes.dtLightnin]
elseif (field[usrIndex].value = 4) then
    perform hero.assign[DamageRes.dtPoison]
endif</eval>
    </thing>
