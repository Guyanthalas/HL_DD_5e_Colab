  <thing id="ra5CCGrtwrmTyp" name="Type" description="The greatwryrm&apos;s damage immunity and the damage type of some of its abilities varies based on the family it comes from." compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="Black"/>
    <arrayval field="usrArray" index="1" value="Blue"/>
    <arrayval field="usrArray" index="2" value="Green"/>
    <arrayval field="usrArray" index="3" value="Red"/>
    <arrayval field="usrArray" index="4" value="White"/>
    <eval phase="First" priority="10"><![CDATA[if (field[usrIndex].value = 0) then
   perform hero.assign[DamageImm.dtAcid]
   hero.childfound[ra5CCGWBrthWp].field[abText].text = "acid"
   perform hero.childfound[ra5CCGWBrthWp].assign[DamageType.dtAcid]
   hero.childfound[ra5CCGWChrFlr].field[abText].text = "acid"
   perform hero.childfound[ra5CCGWChrFlr].assign[DamageType.dtAcid]
elseif (field[usrIndex].value = 1) then
   perform hero.assign[DamageImm.dtLightnin]
   hero.childfound[ra5CCGWBrthWp].field[abText].text = "lightning"
   perform hero.childfound[ra5CCGWBrthWp].assign[DamageType.dtLightnin]
   hero.childfound[ra5CCGWChrFlr].field[abText].text = "lightning"
   perform hero.childfound[ra5CCGWChrFlr].assign[DamageType.dtLightnin]
elseif (field[usrIndex].value = 2) then
   perform hero.assign[DamageImm.dtPoison]
   hero.childfound[ra5CCGWBrthWp].field[abText].text = "poison"
   perform hero.childfound[ra5CCGWBrthWp].assign[DamageType.dtPoison]
   hero.childfound[ra5CCGWChrFlr].field[abText].text = "poison"
