  <thing id="i5CArmSmol" name="Smoldering Armor" description="While worn, wisps of harmless, odorless smoke rise fron its surfaces." compset="MagicArmor" buytemplate="BuyMoney" xactspecial="2">
    <fieldval field="miTypeDet" value="any"/>
    <usesource source="5eXGTECP" parent="p5eBooks" name="Xanathar&apos;s Guide To Everything"/>
