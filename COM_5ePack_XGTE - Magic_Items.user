  <thing id="i5CArmoG" name="Armor of Gleaming" description="This armor is always clean; dirt will not taint it." compset="MagicArmor" buytemplate="BuyMoney" xactspecial="2">
    <fieldval field="miTypeDet" value="medium or heavy"/>
    <usesource source="5eXGTECP"/>
    <tag group="ItemRarity" tag="Common"/>
    <eval phase="Final" priority="20000"><![CDATA[
      ~restrict the list of armor to valid ones only
      gizmo.child[gCustMagic].field[gItemExpr].text = #magicgearexpr[] & " & ((ArmorClass.Medium | ArmorClass.Heavy))"

      ~set our name based on the type of armor chosen
      field[livename].text = "Gleaming "
      perform gizmo.findchild[BaseArmor].setfocus
      if (state.isfocus <> 0) then
        field[livename].text &= titlecase(focus.field[name].text)
      else
        field[livename].text &= "Armor"
        endif
      field[shortname].text = field[livename].text
      field[sbName].text = field[livename].text

      ~show in specials list when equipped
      doneif (field[gIsEquip].value = 0)
      perform assign[Helper.ShowSpec]]]>
      <after name="gItemExpr Set"/>
      </eval>
    <child entity="gMagArmor"></child>
    </thing>
