      <autotag group="Usage" tag="LongRest"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <bootstrap thing="spGustWind">
      <containerreq phase="First" priority="500"><![CDATA[count:Classes.? >= 3]]></containerreq>
