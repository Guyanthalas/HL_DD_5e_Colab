      </bootstrap>
    </thing>
  <thing id="ra5CMPShfBst" name="Beasthide Shifting" description="As a bonus action, you can shift into a more bestial form. The shift lasts for 1 minute, until you die, or until you change back as a bonus action. \n\nWhen you shift, you gain temporary hit points equal to twice your proficiency bonus + 1d6. Also while shifted, you have a +1 bonus to your AC. \n\nYou can shift a number of times equaling your proficiency bonus, regaining all spent uses when you complete a long rest." compset="RaceSpec">
    <fieldval field="actIncrNam" value="+1d6 temp HP"/>
    <fieldval field="actUserMin" value="1"/>
    <fieldval field="actUserMax" value="6"/>
    <fieldval field="abDuration" value="1"/>
    <fieldval field="actName" value="Shifted (bonus action, 1 min)"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="Bonus"/>
    <tag group="abRange" tag="Personal"/>
    <tag group="Helper" tag="UserAdjust" name="UserAdjust" abbrev="UserAdjust"/>
    <tag group="abDuration" tag="Minute"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Usage" tag="LongRest"/>
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="LvNamePar" tag="IncAction"/>
    <bootstrap thing="thp5CEbShtTmpHP"></bootstrap>
    <eval phase="PreAttr" priority="10000"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] <> 0)

      doneif (field[abilActive].value = 0)

      hero.childfound[ArmorClass].field[Bonus].value += 1]]></eval>
    <eval phase="Final" priority="500" index="3"><![CDATA[
      ~ If we're not shown, just get out now
      doneif (tagis[Helper.ShowSpec] = 0)

      ~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      field[trkMax].value = hero.childfound[ProfBonus].field[tProfBonus].value
      field[abValue].value += field[trkMax].value * 2     

      hero.childfound[thp5CEbShtTmpHP].field[thpMinAllw].value = field[abValue].value + field[actUser].value
      hero.childfound[thp5CEbShtTmpHP].field[thpMaxAllw].value = field[abValue].value + field[actUser].value
      ~hero.childfound[thp5CEbShtTmpHP].field[thpCurr].value = field[abValue].value]]></eval>
    </thing>
