  <thing id="ra5CMPTrtNAr" name="Natural Armor" description="Your shell provides you a base AC of 17 (this is unaffected by your Dexterity modifier). You cannot wear light, medium, or heavy armor. However, shields can still provide their bonus to you as normal." compset="RaceSpec">
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
