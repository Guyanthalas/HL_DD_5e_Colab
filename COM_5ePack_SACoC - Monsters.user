  <thing id="ra5CTelpAct" name="Teleport" description="[abText] teleports to an open space visible to and within [abValue] feet of [abText2]." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="FeatureTyp" tag="Action"/>
    <tag group="abAction" tag="Action"/>
    <eval phase="Final" priority="50000"><![CDATA[field[CustDesc].text =  field[abText].text & " teleports to an open space visible to and within " & field[abValue].value & " feet of " & field[abText2].text & "."]]></eval>
    </thing>
  <thing id="ra5CBldAegis" name="Blood Aegis" description="While not wearing armor or wielding a shield, [abText]&apos;s Constitution modifier is included in [abText2] AC." compset="RaceSpec">
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="Final" priority="50000"><![CDATA[field[CustDesc].text =  "While not wearing armor or wielding a shield, " & field[abText].text & "'s Constitution modifier is included in " & field[abText2].text & " AC."]]></eval>
    <eval phase="PostAttr" priority="10000" index="2"><![CDATA[ ~ If we're disabled, do nothing & 
 doneif (tagis[Helper.Disable] = 1)
  if (hero.tagis[Hero.EquipShld] = 0) then
    if (hero.tagis[Hero.EquipArmor] = 0) then
      hero.child[ArmorClass].field[Bonus].value += #attrmod[aCON]
      foreach pick in hero from BaseRace
        eachpick.field[rArmText].text = "blood aegis"
        nexteach
    endif
  endif]]></eval>
    </thing>
  <thing id="ra5CBldBoil" name="Blood Boil" description="Choosing a point within 150 feet of [abText2], [abText] creates a 20-foot radius sphere centered on that point and filled with a burst of searing, blood-red mist. Each creature visible to and chosen by [abText] within that area must make a DC 17 Constitution save. On a failure, a creature suffers 38 (7d10) necrotic damage and is incapacitated until the end of its next turn. On a success, a creature takes half the damage and isn&apos;t incapacitated. A creature dies if reduced to 0 hit points by this necrotic damage." compset="RaceSpec">
    <fieldval field="abRange" value="150"/>
    <tag group="FeatureTyp" tag="Action"/>
    <tag group="abAction" tag="Action"/>
    <tag group="Recharge" tag="4"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="abSave" tag="aCON"/>
    <eval phase="Final" priority="50000"><![CDATA[field[CustDesc].text =  "Choosing a point within 150 feet of " & field[abText2].text & ", " & field[abText].text & " creates a 20-foot radius sphere centered on that point and filled with a burst of searing, blood-red mist. Each creature visible to and chosen by " & field[abText].text & " within that area must make a DC 17 Constitution save. On a failure, a creature suffers 38 (7d10) necrotic damage and is incapacitated until the end of its next turn. On a success, a creature takes half the damage and isn't incapacitated. A creature dies if reduced to 0 hit points by this necrotic damage."]]></eval>
    </thing>
