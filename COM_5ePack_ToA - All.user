  <thing id="ra5cWatDep" name="Water Dependency" description="Each day you must immerse yourself in water for minimum of 1 hour. \nIf you fail to do this you suffer one level of exhaustion at the end of that day. \nTo recover from this exhaustion you must use magical means or immerse yourself in water for minimum of 1 hour." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="c5CChTrsS" name="Charm of Treasure Sense" description="For 3 days, the charm allows you to use an action to determine, by scent, the location of precious metals and stones within 30 feet of you. After that time, the charm goes away." compset="CustomSpec" uniqueness="useronce">
