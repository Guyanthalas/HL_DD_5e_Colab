doneif (field[gIsAttuned].value = 0)

perform hero.assign[DamageRes.dtCold]</eval>
    </thing>
  <thing id="mw5CShatte" name="Shatterspike" description="You have a +1 bonus to attack and damage rolls you make with this magic weapon.\n\nIf {i}Shatterspike{/i} is used to strike an object (such as a door), any hit is automatically considered a critical hit, and it can deal either bludgeoning or slashing damage to the object (your choice). \n\nThe sword is immune from damage from nonmagical sources. \n\n({i}The Sunless Citadel{/i})" compset="MagicWep">
    <fieldval field="miTypeDet" value="longsword"/>
    <fieldval field="Bonus" value="1"/>
    <fieldval field="wAttackEff" value="If it hits an object, the hit is automatically a critical hit, and it can deal bludgeoning or slashing damage to the object (your choice)."/>
    <usesource source="p5eTYPP"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="ItemRarity" tag="Uncommon"/>
    <tag group="IsWeapon" tag="wShortswor" name="Shortsword" abbrev="Shortsword"/>
    <tag group="Helper" tag="NeedAttune" name="Requires Attunement" abbrev="Requires Attunement"/>
    <child entity="wSpecMagic">
      <bootstrap thing="wLongsword"></bootstrap>
      </child>
    </thing>
  <thing id="mw5CSpeBac" name="Spear of Backbiting" description="You gain a +2 bonus to attack and damage rolls made with this magic weapon. Its normal and long ranges are both increased by 30 feet, and it deals one extra die of damage on a hit when thrown. It flies back to your hand immediately after you throw it and it hits or misses, \n\n{b}{i}Curse.{/i}{/b} The weapon is cursed, and becoming attuned to it extends the curse to you. Until the curse is broken with {i}remove curse{/i} or similar magic, you are unwilling to part with the weapon keeping it within reach at all times. In addition, you have disadvantage on attack rolls made with weapons other than this one. \n\nWhenever you roll a 1 on an attack roll using this weapon, the weapon bends or flies to hit you in the back. Make a new attack roll with advantage against your own AC. If the result is a hit, you take damage as if you had attacked yourself with the spear. \n\n({i}Tomb of Horrors{/i})" compset="MagicWep" buytemplate="BuyMoney" xactspecial="2">
    <fieldval field="miTypeDet" value="spear or javelin"/>
    <fieldval field="Bonus" value="2"/>
    <usesource source="p5eTYPP"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="Helper" tag="CursedItem"/>
    <tag group="ItemRarity" tag="VeryRare" name="Very Rare" abbrev="Very Rare"/>
    <tag group="Helper" tag="NeedAttune" name="Requires Attunement" abbrev="Requires Attunement"/>
    <eval phase="Final" priority="20000"><![CDATA[      ~restrict the list of weapons to valid ones only
      gizmo.child[gCustMagic].field[gItemExpr].text = #magicgearexpr[] & " & (thingid.wJavelin | thingid.wSpear)"

      ~set our name based on the type of weapon chosen
      perform gizmo.findchild[BaseWep].setfocus
      if (state.isfocus <> 0) then
        field[livename].text &= titlecase(focus.field[name].text)
      else
        field[livename].text &= "Spear"
        endif
      field[livename].text &= " of Backbiting"
      field[shortname].text = field[livename].text
      field[sbName].text = field[livename].text

doneif (field[gIsAttuned].value = 0)

~ increase thrown range
      field[wRangeNorm].value += 30
      field[wRangeLong].value += 30
~ add extra die of damage
      field[wDieCount].value += 1
      field[wDCntVers].value += 1]]>
      <after name="gItemExpr Set"/>
      </eval>
    <child entity="gMagWeapon"></child>
    </thing>
  <thing id="mi5CStIlLu" name="Stone of Ill Luck" description="Under scrutiny, this appears to be a  {i}stone of good luck{/i} to anyone who tries to identify it, and it grants that item&apos;s properties while on your person. \n\n{b}{i}Curse.{/i}{/b} This item is cursed. While it is carried, you take a -2 penalty to ability checks and saving throws.  You are not willing to part with the stone until the curse is broken with {i}remove curse{/i} or similar magic. \n\n({i}The Hidden Shrine of Tamoachan{/i})" compset="Wondrous">
    <usesource source="p5eTYPP"/>
    <tag group="Helper" tag="CursedItem"/>
    <tag group="Helper" tag="NeedAttune" name="Requires Attunement" abbrev="Requires Attunement"/>
    <tag group="ItemRarity" tag="Uncommon" name="Uncommon" abbrev="Uncommon"/>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      doneif (field[gIsAttuned].value = 0)

      hero.child[svAll].field[Bonus].value += -2
      hero.child[AllSkills].field[Bonus].value += -2

      foreach pick in hero from BaseAttr
        #situational[eachpick,"-2 penalty on ability checks",field[thingname].text]
        nexteach]]></eval>
    </thing>
  <thing id="mw5CWaythe" name="Waythe" description="You gain a +1 bonus to attack and damage rolls made with {i}Waythe{/i}. When you hit a creature with the giant tag, the giant takes an extra 2d6 slashing damage. The target giant must also succeed on a DC 15 Strength saving throw or fall prone. \n\nThe sword also functions as a {i}wand of enemy detection{/i} with 7 charges. {i}Waythe{/i} regains all of its expended charges at dawn and isn&apos;t at risk of crumbling if its last charge is used. \nWhile holding it, you can use an action and expend 1 charge to speak its command word. For the next minute, you know the direction of the nearest creature hostile to you within 60 feet, but not its distance from you. The wand can sense the presence of hostile creatures that are ethereal, invisible, disguised, or hidden, as well as those in plain sight. The effect ends if you stop holding the sword.\n\n{b}{i}Sentience.{/i}{/b} {i}Waythe{/i} is a sentient weapon of neutral good alignment, with an Intelligence of 12, a Wisdom of 2, and a Charisma of 14. It has hearing and darkvision out to 120 feet. \n\nThe weapon can speak and understand Giant and Common, and it can communicate telepathically with its wielder. \n\n{b}{i}Personality.{/i}{/b} This sword believes in freedom and allowing others to live as they see fit. It is very protective of its friends and wants to be wielded bya like-minded individual. (Perk: It takes only 1 minute for any good-aligned character to gain attunement with  {i}Waythe{/i}). The sword is foolhardy and vocally urges bold action. It is likely to come into conflict with either an evil or a timid wielder.\n\n({i}Against the Giants{/i})" compset="MagicWep" buytemplate="BuyMoney" xactspecial="2">
    <fieldval field="miTypeDet" value="greatsword"/>
    <fieldval field="Bonus" value="1"/>
    <fieldval field="wAttackEff" value="If the target is a giant, the sword deals an extra 2d6 slashing damage and the target must succeed at a DC 15 Strength saving throw or fall prone. "/>
    <fieldval field="trkMax" value="7"/>
    <usesource source="p5eTYPP"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="Helper" tag="NeedAttune" name="Requires Attunement" abbrev="Requires Attunement"/>
    <tag group="Usage" tag="Charges"/>
    <tag group="User" tag="Tracker"/>
    <tag group="ItemRarity" tag="Legendary" name="Legendary" abbrev="Legendary"/>
    <child entity="gMagWeapon">
      <bootstrap thing="wGreatswrd"></bootstrap>
      </child>
    </thing>
  <thing id="mw5CEntang" name="Wand of Entangle" description="This wand has 7 charges. While holding it, you can use an action to  to cast the {i}entangle{/i} spell (save DC 13) from it, using 1 charge. \n\nThe wand regains 1d6 + 1 expended charges daily at dawn. When you expend the last charge, roll a d20. On a 1, the wand crumbles into ashes and is destroyed. \n\n({i}The Sunless Citadel{/i})" compset="Wand">
    <fieldval field="miAttunTxt" value="requires attunement by a spellcaster"/>
    <fieldval field="trkMax" value="7"/>
    <usesource source="p5eTYPP"/>
    <tag group="User" tag="Tracker"/>
    <tag group="Usage" tag="Charges"/>
    <tag group="ItemRarity" tag="Uncommon"/>
    <tag group="Helper" tag="NeedAttune"/>
    <bootstrap thing="spEntangle">
      <autotag group="ChargeUse" tag="1"/>
      <autotag group="Helper" tag="ItemSpell"/>
      <autotag group="Usage" tag="Charges"/>
      </bootstrap>
    </thing>
  <thing id="r5CCentMum" name="Centaur Mummy" description="{b}{i}Undead Nature.{/i}{/b} A mummy doesn&apos;t require air, food, drink, or sleep" compset="Race">
    <comment>As seen it carries a Medium sized pike.</comment>
    <fieldval field="rHitDice" value="10"/>
    <fieldval field="rHPStart" value="85"/>
    <fieldval field="rSTR" value="10"/>
    <fieldval field="rDEX" value="2"/>
    <fieldval field="rCON" value="6"/>
    <fieldval field="rINT" value="-5"/>
    <fieldval field="rWIS" value="4"/>
    <fieldval field="rCHA" value="2"/>
    <fieldval field="rAC" value="2"/>
    <fieldval field="rCR" value="6"/>
    <fieldval field="rMultiatt" value="The centaur mummy makes two melee attacks, one with its pike and one with its hooves, or it attacks with its pike and uses Dreadful Glare. "/>
    <usesource source="p5eTYPP"/>
