    <eval phase="Render" priority="100000" index="2"><![CDATA[
      ~By default, we use the tRace herofield. If we want something else, we 
      ~ set it in abText
      var name as string
      if (field[abText].isempty = 0) then
        name = field[abText].text
      else
        name = herofield[tRace].text
        endif
        
      field[CustDesc].text = "The #name# can move through a space as narrow as 1 inch wide without squeezing."
      field[CustDesc].text = replace(field[CustDesc].text,"#name#",name,0)]]></eval>
    </thing>
