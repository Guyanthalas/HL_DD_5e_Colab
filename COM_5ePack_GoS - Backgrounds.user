    <fieldval field="AbilName" value="Fishing Tale"/>
    <fieldval field="StartGear" value="Fishing tackle, a net, a favorite fishing lure or oiled leather wading boots, a set of traveler’s clothes, and a belt pouch containing 10 gp"/>
