      <assignval field="sNameMod" value="Save DC 15"/>
      </bootstrap>
    <eval phase="First"><![CDATA[doneif (tagis[Helper.Disable] <> 0)

perform hero.assign[DamageImm.dtCold]]]></eval>
    </thing>
  <thing id="c5CBlsMrng" name="Blessing of the Morninglord" description="You gain 10 temporary hit points each day at dawn." compset="CustomSpec" uniqueness="unique">
