    <fieldval field="gWeight" value="5"/>
    <usesource source="p5eIDRotFP"/>
    <tag group="Helper" tag="EquipMag"/>
    <tag group="Helper" tag="EquipAvail"/>
    <tag group="GearType" tag="gtAdventGe"/>
    </thing>
