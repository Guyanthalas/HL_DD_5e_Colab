  <thing id="ra5CTmpDbnBr" name="Breath Weapon" description="The dragonborn can use an action to breathe at certain kind of damage in a specific area, depending on what color dragonborn it is. Any creatures in that area must make saving throw, the ability for which is also determined by the color, taking 2d6 of that type of damage on a failure or half that on a success. The dragonborn must complete a short or long rest before using this ability again.\n\nThe dragonborn also gains resistance to the same type of damage." compset="RaceSpec">
    <fieldval field="trkMax" value="1"/>
    <fieldval field="wDieCount" value="2"/>
    <fieldval field="wDieSize" value="6"/>
    <arrayval field="usrArray" index="0" value="Black"/>
    <arrayval field="usrArray" index="1" value="Blue"/>
    <arrayval field="usrArray" index="2" value="Brass"/>
    <arrayval field="usrArray" index="3" value="Bronze"/>
    <arrayval field="usrArray" index="4" value="Copper"/>
    <arrayval field="usrArray" index="5" value="Gold"/>
    <arrayval field="usrArray" index="6" value="Green"/>
    <arrayval field="usrArray" index="7" value="Red"/>
    <arrayval field="usrArray" index="8" value="Silver"/>
    <arrayval field="usrArray" index="9" value="White"/>
    <tag group="Usage" tag="ShortRest" name="/short rest" abbrev="/short rest"/>
    <tag group="User" tag="Tracker" name="tracker" abbrev="tracker"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="StandardDC" tag="aCON" name="Constitution" abbrev="Constitution"/>
    <tag group="abAction" tag="Action"/>
    <tag group="FeatureTyp" tag="Action"/>
    <tag group="User" tag="NoAutoName"/>
    <eval phase="First" index="2"><![CDATA[doneif (tagis[Helper.Disable] <> 0)

var iTyp as string
var iRng as string
var iSav as string
var iRes as string
var iDam as string


if (field[usrIndex].value = 0) then
    iTyp = "dtAcid"
    iRng = "5 by 30 ft. line"
    iSav = "aDEX"
    field[abText2].text = "acid"
elseif (field[usrIndex].value = 1) then
    iTyp = "dtLightnin"
    iRng = "5 by 30 ft. line"
    iSav = "aDEX"
    field[abText2].text = "lightning"
elseif (field[usrIndex].value = 2) then
    iTyp = "dtFire"
    iRng = "5 by 30 ft. line"
    iSav = "aDEX"
    field[abText2].text = "fire"
elseif (field[usrIndex].value = 3) then
    iTyp = "dtLightnin"
    iRng = "5 by 30 ft. line"
    iSav = "aDEX"
    field[abText2].text = "lightning"
elseif (field[usrIndex].value = 4) then
    iTyp = "dtAcid"
    iRng = "5 by 30 ft. line"
    iSav = "aDEX"
    field[abText2].text = "acid"
elseif (field[usrIndex].value = 5) then
    iTyp = "dtFire"
    iRng = "15 ft. cone"
    iSav = "aDEX"
    field[abText2].text = "fire"
elseif (field[usrIndex].value = 6) then
    iTyp = "dtPoison"
    iRng = "15 ft. cone"
    iSav = "aCON"
    field[abText2].text = "poison"
elseif (field[usrIndex].value = 7) then
    iTyp = "dtFire"
    iRng = "15 ft. cone"
    iSav = "aDEX"
    field[abText2].text = "fire"
elseif (field[usrIndex].value = 8) then
    iTyp = "dtCold"
    iRng = "15 ft. cone"
    iSav = "aCON"
    field[abText2].text = "cold"
elseif (field[usrIndex].value = 9) then
    iTyp = "dtCold"
    iRng = "15 ft. cone"
    iSav = "aCON"
    field[abText2].text = "cold"
endif

iRes = "DamageRes." & iTyp
iDam = "DamageType." & iTyp
iSav = "abSave." & iSav

perform assignstr[iDam]
perform assignstr[iSav]
field[abText].text = iRng

foreach pick in hero from BaseRace
    perform eachpick.assignstr[iRes]
    nexteach]]></eval>
    <eval phase="Final"><![CDATA[doneif (tagis[Helper.Disable] <> 0)

field[CustDesc].text = "The dragonborn can use an action to breathe " & field[abText2].text & " in a " & field[abText].text & ". Any creatures in that area must make a DC #StandardDC# #abSave# save, taking 2d6 " & field[abText2].text & " damage on a failure or half that on a success. The dragonborn must complete a short or long rest before using this ability again.\n\nIt also gains resistance to " & field[abText2].text & " damage."]]></eval>
    </thing>
