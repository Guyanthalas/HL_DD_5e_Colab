  <thing id="ra5CUnsHrLmb" name="Limbs Type" description="Roll a d4 to determine which kind of limbs the horror has." compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="Standard"/>
    <arrayval field="usrArray" index="1" value="1 - Bone Blade"/>
    <arrayval field="usrArray" index="2" value="2 - Corrosive Pseudopod"/>
    <arrayval field="usrArray" index="3" value="3 - Grasping Tentacle"/>
    <arrayval field="usrArray" index="4" value="4 - Poisonous Limb"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="First"><![CDATA[if (field[usrIndex].value = 1) then
   perform hero.childfound[wOtherMel].assign[DamTypeOvr.dtSlashing]
   perform hero.childfound[wOtherMel].delete[DamTypeOvr.dtBludgeon]
   hero.childfound[wOtherMel].field[wAttackEff].text = "This attack hits critically on a roll of 19 as well as the regular 20. When it does, it rolls the damage dice three times instead of the normal two times."
   hero.childfound[wOtherMel].field[livename].text = "Bone Blade"
   hero.childfound[wOtherMel].field[sbName].text = "Bone Blade"

elseif (field[usrIndex].value = 2) then
   hero.childfound[wOtherMel].field[wDamExtra].text = " plus 9 (2d8) acid"
   hero.childfound[wOtherMel].field[livename].text = "Corrosive Pseudopod"
   hero.childfound[wOtherMel].field[sbName].text = "Corrosive Pseudopod"

elseif (field[usrIndex].value = 3) then
   hero.childfound[wOtherMel].field[wAttackEff].text = "The target creature is grappled with an escape DC of 16. The limb can only grapple one target at a time."
   hero.childfound[wOtherMel].field[livename].text = "Grasping Tentacle"
   hero.childfound[wOtherMel].field[sbName].text = "Grasping Tentacle"

elseif (field[usrIndex].value = 4) then
   perform hero.childfound[wOtherMel].assign[DamTypeOvr.dtPiercing]
   perform hero.childfound[wOtherMel].delete[DamTypeOvr.dtBludgeon]
   hero.childfound[wOtherMel].field[livename].text = "Poisonous Limb"
   hero.childfound[wOtherMel].field[sbName].text = "Poisonous Limb"
   hero.childfound[wOtherMel].field[wAttackEff].text = "The target must make a DC 15 Constitution save. On a failure, it suffers 7 (2d6) poison damage and is poisoned until the end of its next turn."
endif]]></eval>
    </thing>
