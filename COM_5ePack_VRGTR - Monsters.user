    <fieldval field="wDieSize" value="10"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Legendary"/>
    <tag group="abAction" tag="Action"/>
    <tag group="DamageType" tag="dtBludgeon"/>
    </thing>
  <thing id="ra5CRJgRpdShp" name="Rapid Shaping" description="The juggernaut recharges its Deadly Shaping action and uses it." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Legendary"/>
    <tag group="abAction" tag="Action"/>
    <tag group="LegenAct" tag="3"/>
    </thing>
  <thing id="ra5CRlSShrdPr" name="Shrouded Presence" description="The slasher is immune to any effect that would sense its emotions or read its thoughts, and it cannot be detected by abilities that sense Fiends." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CRlSVanStr" name="Vanishing Strike" description="The slasher makes one Slasher&apos;s Knife attack. After the attack hits or misses, the slasher can teleport up to 30 feet to an open space it can see." compset="RaceSpec">
    <fieldval field="abRange" value="30"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="FeatureTyp" tag="Legendary"/>
    <tag group="abAction" tag="Action"/>
    <tag group="LegenAct" tag="3"/>
    </thing>
  <thing id="ra5CStrgRvCh" name="Ravenous Children" description="The strigoi magically summons 1d4 + 2 stirges (see the Monster Manual for the stat block) in open spaces it can see within 30 feet of it. The stirges are under the strigoi&apos;s control and act immediately after the strigoi in the initiative order. The stirges disappear after 1 hour, when the strigoi dies, or when the strigoi dismisses them (no action needed)." compset="RaceSpec">
    <fieldval field="trkMax" value="1"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="User" tag="Tracker"/>
    <tag group="FeatureTyp" tag="Action"/>
    <tag group="abAction" tag="Action"/>
    <tag group="Usage" tag="Day"/>
    </thing>
  <thing id="ra5CStrgTelep" name="Stirge Telepathy" description="The strigoi can use limited telepathy to magically command any stirge within 120 feet of it." compset="RaceSpec">
    <fieldval field="abRange" value="120"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CSwGLmSp" name="Limited Spell Immunity" description="The swarm automatically succeeds on saves against spells of 3rd level or lower, and the attack rolls of such spells always miss it." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CSwGSplRed" name="Spell Redirection" description="When a spell attack roll misses the swarm, the swarm causes that spell to hit another creature of its choice within 30 feet of it that it can see." compset="RaceSpec">
    <fieldval field="abRange" value="30"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="FeatureTyp" tag="Reaction"/>
    <tag group="abAction" tag="Reaction"/>
    </thing>
  <thing id="ra5CSwSSkel" name="Skeletonize" description="If the swarm begins its turn in the same space as a dead creature that is Large or smaller, the corpse is destroyed, leaving behind only equipment and its skeleton." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CUspkBilBd" name="Bile Body" description="Any creature that touches the horror or hits it with a melee attack suffers 5 (1d10) acid damage." compset="RaceSpec">
    <fieldval field="wDieCount" value="1"/>
    <fieldval field="wDieSize" value="10"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="DamageType" tag="dtAcid"/>
    </thing>
  <thing id="ra5CUspkFrmd" name="Formed by the Mists" description="When created, the horror&apos;s body composition takes one of four forms, which determines certain trains within the horror&apos;s stat block. The forms are Aberrant Armor, Loathsome Limbs, Malleable Mass, or Oozing Organs." compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="Aberrant Armor"/>
    <arrayval field="usrArray" index="1" value="Loathsome Limbs"/>
    <arrayval field="usrArray" index="2" value="Malleable Mass"/>
    <arrayval field="usrArray" index="3" value="Oozing Organs"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <bootstrap thing="ra5CAmporh">
      <containerreq phase="First" priority="100">fieldval:usrIndex = 2</containerreq>
      <assignval field="abText" value="horror"/>
      </bootstrap>
    <bootstrap thing="ra5CUspkBilBd">
      <containerreq phase="First" priority="100">fieldval:usrIndex = 3</containerreq>
      </bootstrap>
    <bootstrap thing="ra5CUspkRelSt">
      <containerreq phase="First" priority="100">fieldval:usrIndex = 1</containerreq>
      </bootstrap>
    <eval phase="First">if (field[usrIndex].value = 0) then
   hero.childfound[r5CUnskHrrr].field[rAC].value += 2
   perform hero.childfound[r5CUnskHrrr].assign[CondImm.pcnPetri]
