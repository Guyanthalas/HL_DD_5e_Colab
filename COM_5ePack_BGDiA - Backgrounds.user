    field[pMaximum].value = 1
elseif (hero.childlives[bck5CChar] <> 0) then
    field[pMaximum].value = 2
elseif (hero.childlives[bck5CCrim] <> 0) then
    field[pMaximum].value = 3
elseif (hero.childlives[bck5CEnte] <> 0) then
    field[pMaximum].value = 4
elseif (hero.childlives[bck5CFolk] <> 0) then
    field[pMaximum].value = 5
