  <thing id="c5CChDahl" name="Dark Gift of Dahlver-Nar" description="Once it has received this dark gift, the beneficiary reincarnates when it dies, as if {i}reincarnate{/i} were cast on it. The new body appears within 10 feet of the old one. Once this reincarnation has occurred three times, the dark gift vanishes.\n\nUntil it reincarnates for the third time, the beneficiary loses all their teeth." compset="CustomSpec" uniqueness="useronce">
    <usesource source="p5eCOSCP"/>
    <tag group="Helper" tag="Quaternary"/>
    <tag group="AllowRCust" tag="cfg5CSupGif" name="Supernatural Gifts" abbrev="Supernatural Gifts"/>
    <tag group="Custom" tag="DrkGftSp"/>
    <bootstrap thing="spReincarn">
      <autotag group="Helper" tag="SpellLike"/>
      <autotag group="Custom" tag="DrkGftSp"/>
      <assignval field="sNameMod" value="self-only, automatic"/>
      <assignval field="trkMax" value="3"/>
      </bootstrap>
    <evalrule phase="First" message="This dark gift has been used. Please remove it from your character." summary="Dark Gift used. Please remove."><![CDATA[@valid = 1

foreach pick in hero from BaseSpell where "thingid.spReincarn & Custom.DrkGftSp"
   if (eachpick.field[trkUser].value = 3) then
     @valid = 0
   endif
   nexteach]]></evalrule>
    </thing>
