  <thing id="c5CInfMchD99" name="99 - Big Hands" description="Your hands feel unnaturally big, imposing disadvantage on Dexterity (Sleight of Hand) checks and Dexterity checks using thieves&apos; tools or artisan&apos;s tools. The first weapon attack roll you make in any encounter is also made with disadvantage." compset="CustomSpec" uniqueness="unique">
    <tag group="AllowRCust" tag="cfg5CInfMch"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="Helper" tag="Secondary" name="Secondary" abbrev="Secondary"/>
    <eval phase="First"><![CDATA[field[livename].text = "Big Hands"

#situational[hero.child[skSleight],"Disadvantage",field[livename].text]]]></eval>
    <eval phase="Render" index="2"><![CDATA[     ~ If we're disabled, do nothing & 
     doneif (tagis[Helper.Disable] = 1)

     perform hero.childfound[gTooThieve].setfocus

     #situational[focus,"Disadvantage",field[livename].text]
     focus.field[CustDesc].text = "{b}Big Hands{/b}: Disadvantage\n\n" & focus.field[descript].text

     var sText as string
     var sText2 as string
     var sText3 as string
     sText = "Disadvantage"

     foreach pick in hero from Tool where "ToolCat.Artisan"
       sText2 = eachpick.tagids[thingid.?]
       foreach pick in hero from Tool where sText2
         #situational[eachpick,sText,field[livename].text]
         sText3 = eachpick.field[descript].text
         eachpick.field[CustDesc].text = "{b}Big Hands{/b}: " & sText & "\n\n" & sText3 
         nexteach
       nexteach]]></eval>
    <eval phase="Final" index="4">field[sbName].text = field[livename].text</eval>
    </thing>
