    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    </thing>
  <thing id="c5CRFBTharSp" name="Tharizdun&apos;s Spark" description="This cultist can use a bonus action to touch one of its simple, martial, or natural weapons. The next creature hit by that weapon must make a DC15 Wisdom save. On a failure, is has disadvantage on all ability checks. The creature can repeat the save at the end of each minute, ending the effect for itself on a successful save." compset="CustomSpec" uniqueness="unique">
    <usesource source="5eMPMotM"/>
    <tag group="Helper" tag="Secondary" name="Secondary" abbrev="Secondary"/>
    <tag group="Recharge" tag="6" name="6" abbrev="6"/>
