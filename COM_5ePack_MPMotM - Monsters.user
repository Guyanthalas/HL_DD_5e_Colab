  <thing id="ra5CRKblDRes" name="Dragon&apos;s Resistance" description="The kobold has resistance to a type of damage based on the color of dragon that it draws its power from (choose or roll 1d10): \n\n{b}1-2:{/b} acid (black or copper)\n{b}3-4:{/b} cold (silver or white)\n{b}5-6:{/b} fire (brass, gold, or red)\n{b}7-8:{/b} lightning (blue or bronze)\n{b}9-10:{/b} poison (green)" compset="RaceSpec">
    <arrayval field="usrArray" index="0" value="Acid"/>
    <arrayval field="usrArray" index="1" value="Cold"/>
    <arrayval field="usrArray" index="2" value="Fire"/>
    <arrayval field="usrArray" index="3" value="Lightning"/>
    <arrayval field="usrArray" index="4" value="Poison"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <tag group="User" tag="NoAutoName"/>
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <eval phase="First" priority="10000"><![CDATA[~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

if (field[usrIndex].value = 0) then
   perform assign[DamageRes.dtAcid]
elseif (field[usrIndex].value = 1) then
   perform assign[DamageRes.dtCold]
elseif (field[usrIndex].value = 2) then
   perform assign[DamageRes.dtFire]
elseif (field[usrIndex].value = 3) then
   perform assign[DamageRes.dtLightnin]
elseif (field[usrIndex].value = 4) then
   perform assign[DamageRes.dtPoison]
endif

perform hero.pushtags[DamageRes.?]]]></eval>
    </thing>
