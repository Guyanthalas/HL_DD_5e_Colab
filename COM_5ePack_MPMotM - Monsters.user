  <thing id="ra5CRDMMLrPI" name="Perceive Interlopers" description="The drow projects her mind throughout her lair, marking any potential threats against her or her followers. Until initiative count 20 of the next round, hostile creatures within the lair can&apos;t become hidden from her and receive no benefit from the invisible condition against her." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRDMMLrSW" name="Spectral Web" description="A glistening spectral spider web erupts from a point the drow can see within 120 feet of her. Each creature within 60 feet of that point must make a DC 19 Dexterity save, becoming restrained for 1 minute on a failure. A creature can repeat the saving throw at the end of each of its turns, ending the effect for itself on a success." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aWIS"/>
    <tag group="abSave" tag="aDEX"/>
    </thing>
  <thing id="ra5CRDMMLrTT" name="Telekinetic Throw" description="The drow targets one creature she can see within 60 feet of her and attempts to expel it from her presence. The target must succeed on a DC 19 Strength save or be thrown 2d6 × 10 feet through the air. A creature crashing into a solid object takes 1d6 bludgeoning damage for every 10 feet it was flung. If released in midair, the creature takes falling damage as normal." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aWIS"/>
    <tag group="abSave" tag="aSTR"/>
    </thing>
  <thing id="ra5CREldLrFW" name="Force Wall" description="The elder brain casts {i}wall of force{/i}." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="SpInfo" tag="spWallForc"/>
    </thing>
  <thing id="ra5CREldLrPA" name="Psionic Anchor" description="The elder brain targets one creature it can sense within 120 feet of it and anchors it psionically. The target must make a DC 18 Charisma save. On a failed save, its speed is reduced to 0, and it can&apos;t teleport. It can repeat the saving throw at the end of each of its turns, ending the effect for itself on a success." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aINT"/>
    <tag group="abSave" tag="aCHA"/>
    </thing>
  <thing id="ra5CREldLrPI" name="Psychic Inspiration" description="The elder brain targets one friendly creature it can sense within 120 feet of it. The target is momentarily inspired, gaining advantage on one attack roll, ability check, or saving throw it makes before the end of its next turn." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRFrzLrCW" name="Conjure Walls and Doors" description="Fraz-Urb&apos;luu causes up to five doors within the lair to turn into walls and an equal number of doors to appear on walls where there previously weren&apos;t any." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRFrzLrPA" name="Psychic Anguish" description="Fraz-Urb&apos;luu creates a wave of anguish. Each creature he can see within the lair must succeed on a DC 23 Wisdom save or suffer 33 (6d10) psychic damage." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="abSave" tag="aWIS"/>
    </thing>
  <thing id="ra5CRFrzLrSm" name="Simulacrum" description="Fraz-Urb&apos;luu chooses one Humanoid within the lair and instantly creates a simulacrum of that creature (as if created by the {i}simulacrum{/i} spell). This simulacrum obeys Fraz-Urb&apos;luu&apos;s commands. On next initiative count 20, the simulacrum is destroyed." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="SpInfo" tag="spSimulacr"/>
    </thing>
  <thing id="ra5CRJuiLrGS" name="Green Slime" description="A green slime (see the DMG) appears on a spot on the ceiling that Juiblex chooses within the lair. It lasts for 1 hour, then disintegrates." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRJuiLrSl" name="Slippery Slime" description="Juiblex slimes a square area, up to 10 feet per side, of ground it can see within the lair. When the slime appears, each creature on it must succeed on a DC 21 Dexterity save or fall prone and slide 10 feet in a random direction (roll a d8 to determine the direction). When a creature enters the area for the first time on a turn or ends its turn there, that creature must make the same save. The slime lasts for 1 hour or until it is burned away with fire. If the slime is set on fire, it takes 1 round to burn away. Any creature that begins its turn in the burning slime takes 22 (4d10) fire damage." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aCON"/>
    <tag group="abSave" tag="aDEX"/>
    </thing>
  <thing id="ra5CRJuiLrSt" name="Sticky Slime" description="Juiblex slimes a square area, up to 10 feet per side, of ground it can see within the lair. When the slime appears, each creature in that area must succeed on a DC 21 Strength save or become restrained. When a creature enters the area for the first time on a turn or ends its turn there, that creature must make the same save. A restrained creature is stuck as long as it stays in the slimy area or until it breaks free. The restrained creature, or another creature that can reach it, can use its action to try to break free and must succeed on a DC 21 Strength check to do so. The slime lasts for 1 hour or until it is burned away with fire. If the slime is set on fire, it takes 1 round to burn away.  Any creature that starts its turn in the burning slime takes 22 (4d10) fire damage." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aCON"/>
    <tag group="abSave" tag="aSTR"/>
    </thing>
  <thing id="ra5CRKrnLrCC" name="Create Comforts" description="The ki-rin conjures up one or more permanent objects made of soft, plant-based material. This can include manufactured objects like pillows, rope, blankets, and clothing. The conjured items cannot collectively fill more than a 20-foot cube. The objects materialize 1 minute later in open spaces of the ki-rin&apos;s choice on the floor of the lair." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRKrnLrCS" name="Create Stone and Metal" description="The ki-rin conjures up one or more temporary objects made of stone or metal. The objects cannot collectively fill more than a 2-foot cube. The objects materialize 1 minute later in open spaces of the ki-rin&apos;s choice on the floor of the lair, and the objects vanish after 1 hour." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRKrnLrCW" name="Create Wood" description="The ki-rin conjures up one or more permanent objects made of wood, or similarly sturdy plant-based material, that cannot collectively fill more than a 10-foot cube. The objects materialize 1 minute later in open spaces of the ki-rin&apos;s choice on the floor of the lair." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRMrkLrCS" name="Cast a Spell" description="The morkoth casts {i}darkness{/i}, {i}dispel magic{/i}, or {i}misty step{/i}, using Intelligence as its spellcasting ability and without spending any spell slots." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="SpInfo" tag="spDarkness"/>
    <tag group="SpInfo" tag="spDispMagi"/>
    <tag group="SpInfo" tag="spMistStep"/>
    </thing>
  <thing id="ra5CRMrkLrHy" name="Hypnotize" description="The morkoth uses its Hypnosis action. The effect can originate at any point within 120 feet of the morkoth, even if the morkoth cannot see that point." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CROrcLrGD" name="Grasp of the Dead" description="Orcus causes skeletal arms to rise from an area on the ground in a 20-foot square that he can see. They last until the next initiative count 20. Each creature in that area when the arms appear must succeed on a DC 23 Strength save or be restrained until the arms disappear or until Orcus releases them (freely, no action needed)." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="abSave" tag="aSTR"/>
    </thing>
  <thing id="ra5CROrcLrDU" name="Deadly Utterance" description="Orcus&apos;s booming voice echoes throughout the lair. His speech causes one creature of his choice to be subjected to {i}power word kill{/i}. Orcus doesn&apos;t have to see the creature, but he must know that the individual is in the lair." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="SpInfo" tag="spPwWoKill"/>
    </thing>
  <thing id="ra5CROrcLrUS" name="Undead Servants" description="Orcus causes up to six corpses within the lair to rise as skeletons, zombies, or ghouls. These undead obey his commands, which he can make telepathically from anywhere in the lair." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRYngLrIP" name="Incite the Pack" description="Until the next initiative count 20, all gnolls and hyenas within the lair are enraged, granting them advantage on melee weapon attack rolls and causing attack rolls to have advantage against them." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRYngLrIS" name="Iron Spike" description="Yeenoghu causes a 5-foot tall, 1-inch diameter iron spike to erupt from the ground at a point he can see within 100 feet of him. Any creature in the space where the spike emerges must make a DC 24 Dexterity save. On a failure, the creature takes 27 (6d8) piercing damage and is restrained by being impaled on the spike. A creature can use its action to remove itself (or a creature it can reach) from the spike, ending the restrained condition." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aSTR"/>
    <tag group="abSave" tag="aDEX"/>
    </thing>
  <thing id="ra5CRYngLrPR" name="Pack Rush" description="Each gnoll or hyena that Yeenoghu can see can take its reaction (if available) to move up to its speed." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRZrlLrFb" name="Fireball" description="Zariel casts {i}fireball{/i}." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="SpInfo" tag="spFireball"/>
    </thing>
  <thing id="ra5CRZrlLrII" name="Infernal Illusions" description="Zariel casts the {i}major image{/i} spell four times, targeting different areas with it. Zariel prefers to create images of intruders&apos; loved ones being burned alive. Zariel doesn&apos;t need to concentrate on these spells, which end on initiative count 20 of the next round. Each creature that can see these illusions must succeed on a DC 26 Wisdom save or become frightened of the illusion for 1 minute. A frightened creature can repeat the saving throw at the end of each of its turns, ending the effect for itself on a success." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="abSave" tag="aWIS"/>
    <tag group="SpInfo" tag="spMajoImag"/>
    </thing>
  <thing id="ra5CRZgtLrRP" name="Rally Plants" description="Up to four plant creatures that are friendly to Zuggtmoy and that Zuggtmoy can see can use their reactions to move up to their speed and make a single weapon attack." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRZgtLrSF" name="Summon Fungi" description="Zuggtmoy causes four gas spores or violet fungi to appear in open spaces that she chooses within the lair. They vanish after 1 hour." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
  <thing id="ra5CRZgtLrUS" name="Unleash Spores" description="Zuggtmoy uses either her Infestation Spores or her Mind Control Spores. Rather than being centered herself, they are centered on a mushroom or other fungus within her lair." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Lair"/>
    <tag group="abAction" tag="Action"/>
    </thing>
