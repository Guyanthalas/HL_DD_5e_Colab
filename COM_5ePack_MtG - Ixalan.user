  <thing id="ra5CAgClmb" name="Agile Climber" description="You have a climbing speed of 25 feet, usable only if you&apos;re not wearing medium or heavy armor." compset="RaceSpec">
    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <tag group="abAction" tag="None" name="No action" abbrev="None"/>
    <tag group="Custom" tag="ClimbRemove"/>
    <tag group="Helper" tag="ShowSpec"/>
    <bootstrap thing="xClimb">
      <autotag group="Value" tag="0"/>
      <autotag group="Custom" tag="ClimbRemove"/>
      </bootstrap>
    <eval phase="First" priority="10001"><![CDATA[var iIsOn as number

if (hero.tagis[Hero.MedArmor] <> 0) then
   iIsOn += 1
   endif
if (hero.tagis[Hero.HeavyArmor] <> 0) then
   iIsOn += 1
   endif

if (iIsOn = 0) then
  foreach pick in hero where "thingid.xClimb & Custom.ClimbRemove"
     perform eachpick.tagreplace[Value.0,Value.25]
     nexteach
  field[abValue].value += 1
  endif]]></eval>
    </thing>
