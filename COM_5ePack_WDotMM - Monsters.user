    </thing>
  <thing id="ra5CAnWWanWon" name="Wand of Wonder" description="The animated wand can expend 1 charge to target a random creature with one of the effects from a standard wand of wonder. Any of those effects that would target the user of a wand instead targets the animated wand itself. The wand crumbles into dust if it is reduced to 0 hit points, destroying it." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Action"/>
    <tag group="abAction" tag="Action"/>
    </thing>
