    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="User" tag="Activation"/>
    <tag group="Helper" tag="ShowSpec"/>
    <bootstrap thing="lCommon">
      <containerreq phase="First" priority="100">fieldval:abilActive = 0</containerreq>
      </bootstrap>
    <bootstrap thing="lCommon">
      <containerreq phase="First" priority="100">fieldval:abilActive = 1</containerreq>
      <autotag group="Helper" tag="CantSpeak"/>
      </bootstrap>
