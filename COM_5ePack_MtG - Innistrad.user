  <thing id="ra5CVmNRegene" name="Regeneration" description="The vampire regains 10 hit points at the beginning of its turn if it has at least 1 hit point. If the vampire takes radiant damage or damage from holy water, this trait doesn't function at the beginning of the vampire's next turn." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
