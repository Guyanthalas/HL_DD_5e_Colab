    <eval phase="First"><![CDATA[     ~ don't add the underdark, urban stuff unless the en31 is checked.
     doneif (hero.tagis[source.EN5_31] = 0)

     ~ add the Urban choice to the end of the usrArray choices.
     ~ <arrayval field="usrArray" index="8" value="Urban"/>
     field[usrArray].arraytext[8] = "Urban"

     field[CustDesc].text = replace(field[CustDesc].text, ", or swamp", ", swamp, underdark, or urban", 1)
     field[CustDesc].text &= "\n\n{b}Underdark{/b}\n{b}Druid Level Circle Spells{/b}\n3rd               {i}spider climb{/i}, {i}web{/i}\n5th               {i}gaseous form{/i}, {i}stinking cloud{/i}\n7th               {i}greater invisibility{/i}, {i}stone shape{/i}\n9th               {i}cloud kill{/i}, {i}insect plague{/i}\n\n{b}Urban{/b}\n{b}Druid Level Circle Spells{/b}\n3rd               {i}calm emotions{/i}, {i}knock{/i}\n5th               {i}glyph of warding{/i}, {i}tongues{/i}\n7th               {i}fabricate{/i}, {i}private sanctum{/i}\n9th               {i}animate objects{/i}, {i}hallow{/i}"]]></eval>
    </thing>
  <thing id="c5CCirUrbn" name="Circle of the Land: Urban" description="While exceedingly rare, some druids view civilization as just another aspect of nature and consider the urban landscape just another environment. While urban druids are identical to other Circle of the Land druids in most respects, they do have their own unique selection of circle spells on which to draw." compset="CustomSpec" uniqueness="unique">
